# Copy this file to traefik/.env on the server and fill in the values.
# ⚠️ Do NOT commit the generated .env file — it contains secrets.

# CF_API_TOKEN should be a Cloudflare token with Zone.DNS:Edit and Zone.Zone:Read permissions.
CF_API_TOKEN=YOUR_CLOUDFLARE_DNS_EDIT_TOKEN

# Email used for Let's Encrypt registration and renewal notifications.
EMAIL=you@example.com

# Set to true for initial tests to avoid Let's Encrypt rate limits.
USE_STAGING=false